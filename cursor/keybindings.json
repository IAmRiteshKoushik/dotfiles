// Place your key bindings in this file to override the defaults
[
    {
        "key": "ctrl+i",
        "command": "workbench.action.quickchat.launchInlineChat",
        "when": "!inChat"
    },
    {
        "key": "ctrl+a",
        "command": "-editor.action.selectAll"
    },
    {
        "key": "ctrl+l",
        "command": "-workbench.action.chat.newEditSession"
    },
    {
        "key": "ctrl+l",
        "command": "-workbench.action.chat.newChat"
    },
    {
        "key": "ctrl+k",
        "command": "-workbench.action.quickchat.launchInlineChat"
    },
    {
        "key": "ctrl+space ctrl+g",
        "command": "periscope.search"
    },
    {
        "key": "ctrl+shift+t",
        "command": "-workbench.action.reopenClosedEditor"
    },
    {
        "key": "ctrl+shift+t",
        "command": "workbench.view.extension.todo-tree-container",
        "when": "viewContainer.workbench.view.extension.todo-tree-container.enabled"
    },
    {
        "key": "ctrl+shift+o",
        "command": "outline.focus"
    },
    {
        "key": "ctrl+e",
        "command": "-editor.action.toggleScreenReaderAccessibilityMode",
        "when": "accessibilityHelpIsShown"
    },
    {
        "key": "ctrl+e",
        "command": "-extension.vim_ctrl+e",
        "when": "editorTextFocus && vim.active && vim.use<C-e> && !inDebugRepl"
    },
    {
        "key": "ctrl+e",
        "command": "-workbench.action.quickOpenNavigateNextInFilePicker",
        "when": "inFilesPicker && inQuickOpen"
    },
    {
        "key": "ctrl+space ctrl+f",
        "command": "workbench.action.quickOpen"
    },
    {
        "key": "ctrl+e",
        "command": "-workbench.action.quickOpen"
    },
    {
        "key": "ctrl+e",
        "command": "workbench.explorer.fileView.focus"
    },
]